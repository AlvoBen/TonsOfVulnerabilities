using System;
using Epi;
using System.Collections.Generic;

namespace Epi.Collections
{



    public class ViewCollection : NamedObjectCollection
    {
        //Public Methods





        public View GetViewById(int viewId)
        {
            foreach (View view in this)
            {
                if (view.Id == viewId)
                {
                    return (view);
                }
            }
            return (null);
        }
        //Public Methods
    }
}
 