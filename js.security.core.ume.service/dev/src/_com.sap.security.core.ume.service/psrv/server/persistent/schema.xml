<schema majorVersion="1" minorVersion="0">
<providerIdentifier providerIDType="urn:oasis:names:tc:SPML:1:0#URN">
	<providerID>SAP</providerID>
</providerIdentifier>
<schemaIdentifier schemaIDType="urn:oasis:names:tc:SPML:1:0#GenericString">
	<schemaID>SAPprincipals</schemaID>
</schemaIdentifier>
<!-- all attribute definitions -->
<attributeDefinition name="logonname" description="Unique name and logonid"/>
<attributeDefinition name="firstname" description="First name"/>
<attributeDefinition name="lastname" description="Last name"/>
<attributeDefinition name="salutation" description="Salutation"/>
<attributeDefinition name="title" description="Title"/>
<attributeDefinition name="jobtitle" description="Title of the job"/>
<attributeDefinition name="mobile" description="Mobile number"/>
<attributeDefinition name="telephone" description="Complete telephone number"/>
<attributeDefinition name="displayname" description="Display name"/>
<attributeDefinition name="description" description="Human readable description"/>
<attributeDefinition name="password" description="Logon password"/>
<attributeDefinition name="oldpassword" description="Logon password"/>
<attributeDefinition name="email" description="Email address"/>
<attributeDefinition name="fax" description="Complete fax number"/>
<attributeDefinition name="locale" description="Locale code"/>
<attributeDefinition name="timezone" description="Timezone"/>
<attributeDefinition name="validfrom" description="Date the user gets valid"/>
<attributeDefinition name="validto" description="Date the user gets invalid"/>
<attributeDefinition name="certificate" description="User certificate (base 64 encoding)"/>
<attributeDefinition name="lastmodifydate" description="Date of last change"/>
<attributeDefinition name="islocked" description="Is user locked" type="xsd:boolean"/>
<attributeDefinition name="ispassworddisabled" description="Is password disabled" type="xsd:boolean"/>
<attributeDefinition name="uniquename" descripton="Unique identifier"/>
<attributeDefinition name="member" description="Assigned members" multivalued="true"/>
<attributeDefinition name="department" description="Department code"/>
<attributeDefinition name="id" description="Backend id"/>
<attributeDefinition name="isserviceuser" description="Specifies if object is a technical user"/>
<attributeDefinition name="securitypolicy" description="Specifies the type of the user (default,technical,unknown)"/>
<attributeDefinition name="datasource" description="Specifies the home data source of the object, readonly"/>
<attributeDefinition name="assignedroles" description="List of all directly assigned roles"/>
<attributeDefinition name="allassignedroles" description="List of all assigned roles, readonly"/>
<attributeDefinition name="assignedgroups" description="List of all directly assigned groups"/>
<attributeDefinition name="allassignedgroups" description="List of all assigned groups, readonly"/>
<attributeDefinition name="distinguishedname" description="Returns the LDAP distinguished name if the object is stored on an LDAP server"/>
<attributeDefinition name="company" description="Name of the assigned company"/>
<attributeDefinition name="streetaddress" description="Home address of the user"/>
<attributeDefinition name="city" description="Name of the city"/>
<attributeDefinition name="zip" description="Postal code of the city"/>
<attributeDefinition name="pobox" description="PO box"/>
<attributeDefinition name="country" description="Contry code following ISO code 3166"/>
<attributeDefinition name="state" description="Name of a state"/>
<attributeDefinition name="orgunit" description="Name of an organization"/>
<attributeDefinition name="accessibilitylevel" description="Accessibility level of the user"/>
<attributeDefinition name="passwordchangerequired" description="Specifies if the provided password is a productive one, can only be set to true if a secure transport layer is used"/>
<objectClassDefinition name="sapuser" description="Representation of user objects of SAP Systems">
	<memberAttributes>
		<attributeDefinitionReference name="logonname">
			<umeMapping type="sapaccount" namespace="default" name="j_user"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="isserviceuser">
			<umeMapping namespace="$serviceUser$" name="SERVICEUSER_ATTRIBUTE"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="firstname">
			<umeMapping namespace="default" name="firstname"/>
		</attributeDefinitionReference>		
		<attributeDefinitionReference name="lastname">
			<umeMapping namespace="default" name="lastname"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="salutation">
			<umeMapping namespace="default" name="salutation"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="title">
			<umeMapping namespace="default" name="title"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="jobtitle">
			<umeMapping namespace="default" name="jobtitle"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="mobile">
			<umeMapping namespace="default" name="mobile"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="displayname">
			<umeMapping namespace="default" name="displayname"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="description"> 
			<umeMapping namespace="default" name="description"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="password">
			<umeMapping type="sapaccount" namespace="default" name="newpassword"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="oldpassword">
			<umeMapping type="sapaccount" namespace="default" name="oldpassword"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="email">
			<umeMapping namespace="default" name="email"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="fax">
			<umeMapping namespace="default" name="fax"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="locale">
			<umeMapping namespace="default" name="locale"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="timezone">
			<umeMapping namespace="default" name="timezone"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="validfrom">
			<umeMapping type="sapaccount" namespace="default" name="validfrom"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="validto">
			<umeMapping type="sapaccount" namespace="default" name="validto"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="certificate">
			<umeMapping type="sapaccount" namespace="default" name="javax.servlet.request.X509Certificate"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="lastmodifydate">
			<umeMapping namespace="default" name="lastmodifydate"/>
			<umeMapping type="sapaccount" namespace="default" name="PRINCIPAL_MODIFY_DATE"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="islocked">
			<umeMapping type="sapaccount" namespace="default" name="islocked"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="ispassworddisabled">
			<umeMapping type="sapaccount" namespace="default" name="ispassworddisabled"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="telephone">
			<umeMapping namespace="default" name="telephone"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="department">
			<umeMapping namespace="default" name="department"/>
		</attributeDefinitionReference>	
		<attributeDefinitionReference name="id">
			<umeMapping namespace="default" name="umeid"/>
			<umeMapping type="sapaccount" namespace="default" name="umeid"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="securitypolicy">
			<umeMapping type="sapaccount" namespace="default" name="SecurityPolicy"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="datasource">
			<umeMapping namespace="default" name="datasource"/>
		</attributeDefinitionReference>		
		<attributeDefinitionReference name="assignedroles">
			<umeMapping namespace="default" name="assignedroles"/>
		</attributeDefinitionReference>			
		<attributeDefinitionReference name="allassignedroles">
			<umeMapping namespace="default" name="allassignedroles"/>
		</attributeDefinitionReference>	
		<attributeDefinitionReference name="assignedgroups">
			<umeMapping namespace="default" name="assignedgroups"/>
		</attributeDefinitionReference>			
		<attributeDefinitionReference name="allassignedgroups">
			<umeMapping namespace="default" name="allassignedgroups"/>
		</attributeDefinitionReference>			
		<attributeDefinitionReference name="company">
			<umeMapping namespace="default" name="company"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="streetaddress">
			<umeMapping namespace="default" name="streetaddress"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="city">
			<umeMapping namespace="default" name="city"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="zip">
			<umeMapping namespace="default" name="zip"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="pobox">
			<umeMapping namespace="default" name="pobox"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="country">
			<umeMapping namespace="default" name="country"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="state">
			<umeMapping namespace="default" name="state"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="orgunit">
			<umeMapping namespace="default" name="orgunit"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="accessibilitylevel"> 
			<umeMapping namespace="default" name="accessibilitylevel"/>
		</attributeDefinitionReference>
		<attributeDefinitionReference name="passwordchangerequired">
			<umeMapping type="sapaccount" namespace="default" name="passwordchangerequired"/>
		</attributeDefinitionReference>
	</memberAttributes>
</objectClassDefinition>

<objectClassDefinition name="saprole" description="Representation of role objects of SAP Systems">
	<memberAttributes>
        <attributeDefinitionReference name="member">
        	<umeMapping namespace="default_relation" name="PRINCIPAL_RELATION_MEMBER_ATTRIBUTE"/>
        </attributeDefinitionReference>
        <attributeDefinitionReference name="uniquename" required="true">
        	<umeMapping namespace="default" name="uniquename"/>
        </attributeDefinitionReference>
        <attributeDefinitionReference name="displayname" required="true">
                	<umeMapping namespace="default" name="displayname"/>
        </attributeDefinitionReference>
        <attributeDefinitionReference name="description">
	        <umeMapping namespace="default" name="description"/>
	    </attributeDefinitionReference>
        <attributeDefinitionReference name="lastmodifydate">
        	<umeMapping namespace="default" name="PRINCIPAL_MODIFY_DATE"/>
        </attributeDefinitionReference>
        <attributeDefinitionReference name="id">
			<umeMapping namespace="default" name="umeid"/>
		</attributeDefinitionReference>
	<attributeDefinitionReference name="datasource">
		<umeMapping namespace="default" name="datasource"/>
	</attributeDefinitionReference>			
	</memberAttributes>
</objectClassDefinition>

<objectClassDefinition name="sapgroup" description="Representation of group objects of SAP Systems">
	<memberAttributes>
        <attributeDefinitionReference name="member">
        	<umeMapping namespace="default_relation" name="PRINCIPAL_RELATION_MEMBER_ATTRIBUTE"/>
        </attributeDefinitionReference>
        <attributeDefinitionReference name="uniquename" required="true">
        	<umeMapping namespace="default" name="uniquename"/>
        </attributeDefinitionReference>
        <attributeDefinitionReference name="displayname" required="true">
                	<umeMapping namespace="default" name="displayname"/>
        </attributeDefinitionReference>
        <attributeDefinitionReference name="description">
	        <umeMapping namespace="default" name="description"/>
	    </attributeDefinitionReference>
        <attributeDefinitionReference name="lastmodifydate">
        	<umeMapping namespace="default" name="PRINCIPAL_MODIFY_DATE"/>
        </attributeDefinitionReference>
        <attributeDefinitionReference name="id">
			<umeMapping namespace="default" name="umeid"/>
		</attributeDefinitionReference>
	<attributeDefinitionReference name="datasource">
		<umeMapping namespace="default" name="datasource"/>
	</attributeDefinitionReference>			
	<attributeDefinitionReference name="assignedroles">
		<umeMapping namespace="default" name="assignedroles"/>
	</attributeDefinitionReference>			
	<attributeDefinitionReference name="allassignedroles">
		<umeMapping namespace="default" name="allassignedroles"/>
	</attributeDefinitionReference>			
	<attributeDefinitionReference name="distinguishedname">
		<umeMapping namespace="com.sap.security.core.bridge" name="dn"/>
	</attributeDefinitionReference>	
	</memberAttributes>
</objectClassDefinition>

</schema>