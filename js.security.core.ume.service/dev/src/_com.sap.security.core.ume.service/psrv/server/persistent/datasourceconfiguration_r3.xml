<dataSources>
  <dataSource id="PRIVATE_DATASOURCE" className="com.sap.security.core.persistence.datasource.imp.DataBasePersistence" isReadonly="false" isPrimary="true">
    <homeFor>
      <principals>
        <principal type="group"/>
        <principal type="user"/>
        <principal type="account"/>
        <principal type="team"/>
        <principal type="ROOT"/>
        <principal type="OOOO"/>
      </principals>
    </homeFor>
    <notHomeFor/>
    <responsibleFor>
      <principals>
        <principal type="group"/>
        <principal type="user"/>
        <principal type="account"/>
        <principal type="team"/>
        <principal type="ROOT"/>
        <principal type="OOOO"/>
      </principals>
    </responsibleFor>
    <privateSection/>
  </dataSource>
  <dataSource id="R3_DATASOURCE" className="com.sap.security.core.persistence.datasource.imp.R3Persistence" isReadonly="false" isPrimary="true" policyEnforced="true">
    <homeFor/>
    <responsibleFor>
      <principals>
        <principal type="user">
          <nameSpaces>
            <nameSpace name="com.sap.security.core.usermanagement">
              <attributes>
                <attribute name="uniquename" readonly="true"/>
                <attribute name="firstname" readonly="true"/>
                <attribute name="lastname" readonly="true"/>
                <attribute name="salutation" readonly="true"/>
                <attribute name="title" readonly="true"/>
                <attribute name="jobtitle" readonly="true"/>
                <attribute name="department" readonly="true"/>
                <attribute name="email" readonly="true"/>
                <attribute name="telephone" readonly="true"/>
                <attribute name="mobile" readonly="true"/>
                <attribute name="fax" readonly="true"/>
                <attribute name="locale" readonly="true"/>
                <attribute name="timezone" readonly="true"/>
                <attribute name="referenceuser" readonly="true"/>
                <attribute name="personuuid" readonly="true"/>
              </attributes>
            </nameSpace>
          </nameSpaces>
        </principal>
        <principal type="account">
          <nameSpaces>
            <nameSpace name="com.sap.security.core.usermanagement">
              <attributes>
                <attribute name="j_user" readonly="true"/>
                <attribute name="j_password"/>
                <attribute name="validfrom" readonly="true"/>
                <attribute name="validto" readonly="true"/>
                <attribute name="islocked" readonly="true"/>
                <attribute name="lockreason" readonly="true"/>
                <attribute name="passwordchangerequired"/>
                <attribute name="userid" readonly="true"/>
                <attribute name="ispassworddisabled"/>
                <attribute name="logonalias" readonly="true"/>
                <attribute name="SecurityPolicy" readonly="true"/>
              </attributes>
            </nameSpace>
          </nameSpaces>
        </principal>
      </principals>
    </responsibleFor>
    <attributeMapping/>
    <privateSection/>
  </dataSource>
</dataSources>
