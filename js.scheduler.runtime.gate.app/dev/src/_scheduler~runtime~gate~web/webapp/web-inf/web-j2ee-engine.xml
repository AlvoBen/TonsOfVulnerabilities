<?xml version="1.0" encoding="UTF-8"?>
<web-j2ee-engine xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="web-j2ee-engine.xsd">
	<spec-version>2.4</spec-version>
	
	<server-component-ref>
  		<name>scheduler/jert</name>
  		<type>INTERFACE</type>
  		<jndi-name>java_scheduler/jert</jndi-name>
	</server-component-ref>
	
	<security-role-map>
        	<role-name>JXBPUsers</role-name>
        	<!-- The created users for JXBP are in group Everyone.
        	      Authentication/Authorization is done by the Scheduler itself -->
        	<server-role-name>Everyone</server-role-name>
      	</security-role-map>

</web-j2ee-engine>
