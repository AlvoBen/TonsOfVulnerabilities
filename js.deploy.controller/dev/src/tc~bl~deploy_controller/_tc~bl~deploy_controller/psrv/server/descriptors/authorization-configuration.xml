<BUSINESSSERVICE NAME="DC">
  <DESCRIPTION LOCALE="en" VALUE="DC Service"/>
  <ACTION NAME="dc_action">
     <DESCRIPTION LOCALE="en" VALUE="Allows deployment functionality in the J2EE Engine."/>
     <PERMISSION CLASS="com.sap.engine.interfaces.security.ServiceAccessPermission" NAME="dc"/>
  </ACTION>
  <ACTION NAME="dc_offline_action">
     <DESCRIPTION LOCALE="en" VALUE="Allows deployment of offline components in the J2EE Engine."/>
     <PERMISSION CLASS="com.sap.engine.interfaces.security.ServiceAccessPermission" NAME="dc_offline"/>
  </ACTION>
  <ROLE NAME="Administrator">
     <ASSIGNEDACTION NAME="dc_action"/>
     <ASSIGNEDACTION NAME="dc_offline_action"/>
  </ROLE>
</BUSINESSSERVICE>