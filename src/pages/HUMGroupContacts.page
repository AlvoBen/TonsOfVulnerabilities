<apex:page showHeader="false" Controller="HUMGroupContactsController" action="{!getGroupContactDetails}" id="intGroupContacts">
	<apex:sectionHeader title="{!oGroupAccount.name}" rendered="{!NOT(ISNULL(oGroupAccount))}"/>
	<apex:pageblock mode="maindetail"> 
	    <apex:outputpanel layout="block" style="width:1000px;">
	         <apex:pageBlockSection columns="1" collapsible="false" showHeader="true" >
	            <apex:outputLabel title="GroupContact Detail" value="Group Contact Details"  style="float:center;"/>
	        </apex:pageBlockSection>
	    </apex:outputpanel>
	</apex:pageblock>
	<apex:pageBlock id="pbPageGC" mode="maindetail">
	    <apex:outputpanel layout="block" style="width:1000px;">
	        <apex:pageMessages ></apex:pageMessages>             
	        <apex:pageBlockSection columns="2" collapsible="false" rendered="{!NOT(ISNULL(oGroupAccount))}">                                                                   
	               <apex:pageBlockSectionItem >                                           
	                   <apex:outputLabel value="{!$Label.HUMGroupContactName}" style="margin-right:0.4em;"/>
	                    <apex:outputPanel id="thePane2" layout="block">
	                           <apex:outputField value="{!oGroupAccount.Group_Contact_Name__c}"/>
	                       </apex:outputPanel>                        
	                </apex:pageBlockSectionItem>
	                <apex:pageBlockSectionItem >
	                    <apex:outputLabel value="{!$Label.HUMGroupContactFax}" style="margin-right:0.4em;"/>
	                    <apex:outputField value="{!oGroupAccount.Fax}"/>
	                </apex:pageBlockSectionItem>                                                                    
	                <apex:pageBlockSectionItem >
	                    <apex:outputLabel value="{!$Label.HUMGroupContactPhone}" style="margin-right:0.4em;"/>
	                    <apex:outputField value="{!oGroupAccount.phone}"/>
	                </apex:pageBlockSectionItem>
	                <apex:pageBlockSectionItem >
	                    <apex:outputLabel value="{!$Label.HUMGroupContactEmail}" style="margin-right:0.4em;"/>
	                    <apex:outputField value="{!oGroupAccount.Work_Email__c}"/>
	                </apex:pageBlockSectionItem>
	                <apex:pageBlockSectionItem >
	                    <apex:outputLabel value="{!$Label.HUMGroupContactPhone_Ext}" style="margin-right:0.4em;"/>
	                    <apex:outputField value="{!oGroupAccount.Phone_Ext__c}"/>
	                </apex:pageBlockSectionItem>                   
	                <apex:pageBlockSectionItem >                             
	                </apex:pageBlockSectionItem>                        
	                <apex:pageBlockSectionItem >
	                    <apex:outputLabel value="{!$Label.HUMGroupContactAddress}" style="margin-right:0.4em;"/>                           
	                        <apex:outputPanel id="thePanel" layout="block">                               
	                            <apex:outputField value="{!oGroupAccount.BillingStreet}"/><p/>
	                            <apex:outputField value="{!oGroupAccount.BillingCity}"/>,&nbsp;
	                            <apex:outputField value="{!oGroupAccount.BillingState}"/>&nbsp;
	                            <apex:outputField value="{!oGroupAccount.BillingPostalCode}"/><p/>                             
	                            <apex:outputField value="{!oGroupAccount.Billingcountry}"/><p/>                                                                                                         
	                        </apex:outputPanel>
	                </apex:pageBlockSectionItem>                   
	           </apex:pageBlockSection>
	    </apex:outputpanel>                                                       
	</apex:pageBlock>
</apex:page>