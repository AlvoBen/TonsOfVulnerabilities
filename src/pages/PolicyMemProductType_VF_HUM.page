<apex:page standardController="Policy_Member__c" extensions="ProductType_C_HUM" showHeader="false">
    <apex:stylesheet value="{!URLFOR($Resource.MouseHoverPopUp_SR_HUM,'/MouseHoverPopUp_SR_HUM/main_css.css')}"/>
    <apex:includeScript value="{!URLFOR($Resource.MouseHoverPopUp_SR_HUM,'/MouseHoverPopUp_SR_HUM/PopUp_File.js')}"/>
    <apex:pageblock mode="maindetail">
    <apex:pageblocksection columns="2">
    <apex:pageblocksectionitem >
    <apex:outputLabel value="{!sLabelValue}" styleClass="LabelFont"></apex:outputLabel>
    <apex:outputPanel >
        <span class="vfHelpText">
                        <apex:outputlink styleclass="noLink" onmouseover="showVisionHover('{!sRecordID}','{!sProductType}', '{!sProductValue}','{!sMessageValue}');" value="javascript:void(0);">
                            {!sProductType}
                            <span id="{!sRecordID}"></span>    
                        </apex:outputlink>
                    </span>
        </apex:outputPanel>
    </apex:pageblocksectionitem>
        <apex:pageBlockSectionItem labelStyleClass="LabelClass" dataStyleClass="LabelClass" >
           <apex:outputText >&nbsp;&nbsp;</apex:outputText>
           <apex:outputText >&nbsp;&nbsp;</apex:outputText>
        </apex:pageBlockSectionItem>  
       
    </apex:pageblocksection>
    </apex:pageblock> 
</apex:page>