-->
<apex:page extensions="HUMLaunchGCPFromCaseController" showHeader="false" sidebar="false" standardController="Case">
  <div style="display: hidden;">
    <form action="{!sGCPUrl}" method="post" name="input" id= "gcp_form">

      <INPUT ID="CaseId" NAME="CaseId" TYPE="hidden" VALUE="{!case.CaseNumber}"/>
      <INPUT ID="CaseNavigationId" NAME="CaseNavigationId" TYPE="hidden" VALUE="{!case.Id}"/>
      <INPUT ID="InteractionId" NAME="InteractionId" TYPE="hidden" VALUE="{!case.Interaction_ID__c}"/>
      <INPUT ID="CallingApplication" NAME="CallingApplication" TYPE="hidden" VALUE="{!sCallingApplication}"/>
      <INPUT ID="CaseOrigin" NAME="CaseOrigin" TYPE="hidden" VALUE="{!sCaseOrigin}"/>
      <INPUT ID="Mode" NAME="Mode" TYPE="hidden" VALUE="{!sMode}"/>
      <INPUT ID="AccountNavigationId" NAME="AccountNavigationId" TYPE="hidden" VALUE="{!Case.AccountId}"/>
      <INPUT ID="InteractingAboutId" NAME="InteractingAboutId" TYPE="hidden" VALUE="{!sInteractingAboutId}"/>
      <INPUT ID="InteractingAboutType" NAME="InteractingAboutType" TYPE="hidden" VALUE="{!sInteractingAboutType}"/>
      <INPUT ID="InteractingWithId" NAME="InteractingWithId" TYPE="hidden" VALUE="{!sInteractingWithId}"/>
      <INPUT ID="InteractingWithType" NAME="InteractingWithType" TYPE="hidden" VALUE="{!sInteractingWithType}"/>
      <INPUT ID="InteractingAboutMemberCoverageEffectiveDate" NAME="InteractingAboutMemberCoverageEffectiveDate" TYPE="hidden" VALUE="{!sInteractingAboutCovergeEffDate}"/>
      <INPUT ID="InteractingWithMemberCoverageEffectiveDate" NAME="InteractingWithMemberCoverageEffectiveDate" TYPE="hidden" VALUE="{!sInteractingWithCovergeEffDate}"/>
      <INPUT ID="InteractingAboutMemberIdBase" NAME="InteractingAboutMemberIdBase" TYPE="hidden" VALUE="{!sInteractingAboutMemberIdBase}"/>
      <INPUT ID="InteractingWithMemberIdBase" NAME="InteractingWithMemberIdBase" TYPE="hidden" VALUE="{!sInteractingWithMemberIdBase}"/>
      <INPUT ID="IsPolicyMemberPresent " NAME="IsPolicyMemberPresent" TYPE="hidden" VALUE="{!bIsPolicyMemberPresent}"/>
      <INPUT ID="InteractingAboutGroupId" NAME="InteractingAboutGroupId" TYPE="hidden" VALUE="{!sInteractingAboutGroupId}"/>
      <INPUT ID="InteractingWithGroupId" NAME="InteractingWithGroupId" TYPE="hidden" VALUE="{!sInteractingWithGroupId}"/>
      <INPUT ID="InteractingAboutPlatform" NAME="InteractingAboutPlatform" TYPE="hidden" VALUE="{!sInteractingAboutPlatform}"/>
      <INPUT ID="InteractingWithPlatform" NAME="InteractingWithPlatform" TYPE="hidden" VALUE="{!sInteractingWithPlatform}"/>
      <INPUT ID="InteractingAboutSubType" NAME="InteractingAboutSubType" TYPE="hidden" VALUE="{!sInteractingAboutSubType}"/>
      <INPUT ID="InteractingWithSubType" NAME="InteractingWithSubType" TYPE="hidden" VALUE="{!sInteractingWithSubType}"/>
      <INPUT ID="InteractingAboutAgentId" NAME="InteractingAboutAgentId" TYPE="hidden" VALUE="{!sInteractingAboutAgentId}"/>
      <INPUT ID="InteractingWithAgentId" NAME="InteractingWithAgentId" TYPE="hidden" VALUE="{!sInteractingWithAgentId}"/>
      <INPUT ID="InteractingAboutProviderId" NAME="InteractingAboutProviderId" TYPE="hidden" VALUE="{!sInteractingAboutProviderId}"/>
      <INPUT ID="InteractingWithProviderId" NAME="InteractingWithProviderId" TYPE="hidden" VALUE="{!sInteractingWithProviderId}"/>
      <INPUT ID="InteractingAboutSourcePlatformCd" NAME="InteractingAboutSourcePlatformCd" TYPE="hidden" VALUE="{!sInteractingAboutSourcePlatformCd}"/>
      <INPUT ID="InteractingWithSourcePlatformCd" NAME="InteractingWithSourcePlatformCd" TYPE="hidden" VALUE="{!sInteractingWithSourcePlatformCd}"/>
      <INPUT ID="InteractingAboutProviderFirstName" NAME="InteractingAboutProviderFirstName" TYPE="hidden" VALUE="{!sInteractingAboutProvFirstName}"/>
      <INPUT ID="InteractingAboutProviderLastName" NAME="InteractingAboutProviderLastName" TYPE="hidden" VALUE="{!sInteractingAboutProvLastName}"/>
      <INPUT ID="InteractingAboutProviderFullName" NAME="InteractingAboutProviderFullName" TYPE="hidden" VALUE="{!sInteractingAboutProvFullName}"/>
      <INPUT ID="InteractingWithProviderFirstName" NAME="InteractingWithProviderFirstName" TYPE="hidden" VALUE="{!sInteractingWithProvFirstName}"/>
      <INPUT ID="InteractingWithProviderLastName" NAME="InteractingWithProviderLastName" TYPE="hidden" VALUE="{!sInteractingWithProvLastName}"/>
      <INPUT ID="InteractingWithProviderFullName" NAME="InteractingWithProviderFullName" TYPE="hidden" VALUE="{!sInteractingWithProvFullName}"/>
      <INPUT ID="InteractingWithFirstName" NAME="InteractingWithFirstName" TYPE="hidden" VALUE="{!sInteractingWithFirstName}"/>
      <INPUT ID="InteractingWithLastName" NAME="InteractingWithLastName" TYPE="hidden" VALUE="{!sInteractingWithLastName}"/>
      <INPUT ID="InteractingWithDateOfBirth" NAME="InteractingWithDateOfBirth" TYPE="hidden" VALUE="{!sInteractingWithDateOfBirth}"/>
      <INPUT ID="InteractingWithGroupName" NAME="InteractingWithGroupName" TYPE="hidden" VALUE="{!sInteractingWithGroupName}"/>
      <INPUT ID="AccountTabId" NAME="AccountTabId" TYPE="hidden" VALUE="{!$CurrentPage.Parameters.aID}"/>
      <INPUT ID="CaseTabId" NAME="CaseTabId" TYPE="hidden" VALUE="{!$CurrentPage.Parameters.cID}"/>
      <br/><br/>

    </form>
    <center> <apex:outputText value="{!$Label.HUMLaunchGCPNavigate}"/> </center>
    <script language="javascript">
      document.getElementById("gcp_form").submit();
    </script>
  </div>
  <apex:form id="myform">
    <!-- For Constructor Related exception -->
    <c:HUMConstructorExceptionComponent errorLogs="{!listErrorLogs}" />
  </apex:form>
</apex:page>