<apex:page sidebar="false" controller="ClaimStatementsDocument_C_HUM" docType="html-5.0" showHeader="false" id="pgId">
 <head>
  <apex:includeScript value="{!URLFOR($Resource.CRM_JQUERY, 'jquery.min.js')}" />  
  <apex:includeScript value="{!URLFOR($Resource.CRM_PLATFORM_TOOLKIT,'/FileSaver.js')}"/>
  <apex:includeScript value="{!$Resource.ClaimStatementsDocument_JS_HUM}"/>
 </head>
 <body>
  <apex:form id="theForm" >
   <apex:actionFunction id="initiate" name="init" action="{!initiateDocumentRequest}" oncomplete="getDocument('{!JSENCODE(sDocumentReturn)}', '{!JSENCODE(IF(bShowServiceError,"true", "false"))}');" reRender="pageMsgOPId" />
    <apex:outputPanel id="pageMsgOPId">
            <apex:pageMessages id="serviceError" rendered="{!bShowServiceError}" ></apex:pageMessages>
    </apex:outputPanel> 
    </apex:form>
  </body>  
</apex:page>