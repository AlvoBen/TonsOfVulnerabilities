<apex:page standardController="Case" extensions="HUMMedicareReinstatProcessController">
  <apex:includeScript value="/support/console/31.0/integration.js" />
  <apex:includeScript value="{!$Resource.HUMUnSavedChangesJS}"/>
  <apex:stylesheet value="{!$Resource.HUMFlowsWordWrap}"/>
  <script >
        
        window.onload =   testSetTabUnsavedChanges('Medicare Good Cause Reinstatement');   
  </script>
  <apex:outputpanel >
    <style type="text/css">
        .FlowPageBlockBtns {
            text-align: left !important;
        }
    </style>
  </apex:outputpanel>
  <br/>
    <apex:outputLabel value="{!$Label.HUMMediGoodCauseReinstatementForm}" style="font-size:16px;font-weight:bold;"/>
  <br/>
  <flow:interview id="MGCR" name="Medicare_Good_Cause_Reinstatement_Flow"   rerender="MGCR" rendered="{!bSubmissionPres}"  finishLocation="{!FinishPage}">
        <apex:param name="WhoId" value="{!sSelectedCaseId}"/>
        <apex:param name="WhatType" value="Case"/>
        <apex:param name="TemplateSubmissionId" value="{!SubmissionId}"/>
        <apex:param name="GCRPersonNameParam" value="{!gcrPersonName}"/>
        <apex:param name="GCRRelationshipParam" value="{!gcrRelationship}"/>
 </flow:interview>
 <flow:interview id="MGCR1" name="Medicare_Good_Cause_Reinstatement_Flow" rerender="MGCR1" rendered="{!!bSubmissionPres}"  finishLocation="{!FinishPage}">
        <apex:param name="WhoId" value="{!sSelectedCaseId}"/>
        <apex:param name="WhatType" value="Case"/>
        <apex:param name="GCRPersonNameParam" value="{!gcrPersonName}"/>
        <apex:param name="GCRRelationshipParam" value="{!gcrRelationship}"/>
    </flow:interview>
</apex:page>