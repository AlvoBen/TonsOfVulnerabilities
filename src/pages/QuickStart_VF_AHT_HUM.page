                                        <div id="div-alert-InfoHeaderVCMessages">{!$Label.QUICKSTART_VERBAL_MESSAGE_HUM}</div>
                                    </div>
                                </div>    
                                <div id="div-alert-InfoHeaderMessage-show" class="alert" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="InfoHeaderMessage-show-image" value="/img/msg_icons/info24.png" />
                                    </div>
                                    <div id="info-autoLogging" class="div-alert-messagecontainer">
                                        <b>Info: </b>
                                        <div id="div-alert-InfoHeaderMessage"></div>
                                    </div>
                                </div>
                                <div id="div-alert-existing-classification-New" class="alertGuidance alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="Warning-existing-classification-New" value="/img/msg_icons/info24.png"/>
                                    </div>
                                    <div class="div-alert-inputmessagecontainer">
                                        
                                        <b>Info: </b><br/>{!$Label.QUICKSTART_DUPLICATE_CLASSIFICATION_HUM}&nbsp;&nbsp;
                                        <apex:outputPanel id="duplicateClassificationPanel">
                                            <apex:repeat id="cLinksId" value="{!lstDupClassificationCases}" var="oLink" >
                                                <a href="#" class="linkDocuments" id="case-link-{!oLink.Id}" style="margin-left: 0px; !important" onclick="openCaseInSubTab('{!oLink.Id}','{!oLink.Title}')" onmouseover="showCaseComment('{!oLink.Id}',this.id);" onmouseout="hidecasecomment();">{!oLink.Title}</a>
                                            </apex:repeat>
                                        </apex:outputPanel>
                                    </div>
                                </div>   
