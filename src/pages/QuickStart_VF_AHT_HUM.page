                                        <apex:image id="WarningHeaderMessage-show-image" value="/img/msg_icons/warning24.png"/>
                                    </div>
                                    <div class="div-alert-inputmessagecontainer">
                                        <b>Warning: </b><br/>
                                        <div id="div-alert-WarningHeaderMessage"></div>
                                        <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="yesAutoLog();">Yes</a>
                                        <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="noAutoLog();">No</a>
                                    </div>
                                </div>
                                <div id="div-alert-InfoHeaderVCMessages-showVerbalConsent" class="alert" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="InfoHeaderVCMessages-showVerbalConsent-image" value="/img/msg_icons/info24.png" />
                                    </div>
                                    <div id="info-VerbalConsent" class="div-alert-messagecontainer">
