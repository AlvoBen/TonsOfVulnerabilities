<apex:page controller="QuickStart_C_AHT_HUM" showheader="false" sidebar="false" standardstylesheets="false" docType="html-5.0"> 
    <meta content="maximum-scale=1.0, initial-scale=1.0, width=device-width" name="viewport" />
    <apex:includescript value="/support/console/36.0/integration.js" /> 
    <apex:includescript value="{!URLFOR($Resource.CRM_JQUERY, 'jquery.min.js')}" /> 
    <apex:includescript value="{!URLFOR($Resource.CRM_DataTables_1_13_4, 'datatables.min.js')}" />
    <link rel="stylesheet" type="text/css" href="{!URLFOR($Resource.CRM_DataTables_1_13_4, 'datatables.min.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.CRM_PLATFORM_TOOLKIT, 'datatables_HUM.min.css')}" />
    <apex:stylesheet value="{!URLFOR($Resource.QuickStart_SR_AHT_HUM, 'QuickStart_SR_AHT_HUM/styles/QuickStart_VF_Style.css')}"/>
    <apex:includescript value="{!URLFOR($Resource.QuickStart_SR_AHT_HUM,'QuickStart_SR_AHT_HUM/scripts/QuickStart_VF_Script.js')}"/>

    <apex:stylesheet value="{!URLFOR($Resource.QuickStart_SR_AHT_HUM, 'QuickStart_SR_AHT_HUM/styles/QuickStart_CMP_Style.css')}"/>
    <apex:includescript value="{!URLFOR($Resource.QuickStart_SR_AHT_HUM,'QuickStart_SR_AHT_HUM/scripts/QuickStart_CMP_Script.js')}"/>
    <apex:includescript value="{!URLFOR($Resource.QuickStart_SR_AHT_HUM,'QuickStart_SR_AHT_HUM/scripts/QuickStart_CMP_Pretext_Script.js')}"/>
    <apex:includeScript value="{!URLFOR($Resource.CRM_JQUERY,'bootstrap-datepicker-uni.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.DatePickerOverrides_SR_HUM, '/js/datePickerOverrides.js')}"/> 
    <link rel="stylesheet" text="text/css" href="{!URLFOR($Resource.CRM_JQUERY, 'bootstrap-datepicker-uni.css')}" /> 
    <apex:stylesheet value="{!URLFOR($Resource.DatePickerOverrides_SR_HUM,'/css/datePickerOverrides.css')}"/>
    <link rel="stylesheet" href="{!URLFOR($Resource.CRM_JQUERY, 'jquery-ui.min.css')}"/>
    <apex:includeScript value="{!URLFOR($Resource.CRM_JQUERY, 'jquery-ui.min.js')}"/>
    
    <div class="row">
        <div id="div-container-page">
            <div id="div-col-header" class="col-xs-12">
                <div id="div-panel-header" class="panel panel-default"> 
                    <div class="panel-heading">
                        <table align="center" width="100%" border="0">
                            <tr>
                                <td align="left"><h3 class="panel-title">{!sQSLabel}</h3></td>
                                <td align="right" >
                               		<apex:outputPanel id="pretextdisabledPanel" rendered="{!NOT(bPreTextConfigured)}">
                                        <h3 class="panel-title" style="height: 5px; margin-top: 12px; margin-right: 15px; margin-left: 250px;">Pretext: Disabled</h3>
                                        <apex:image title="{!$Label.QUICKSTART_PRETEXTCONFIGURE_HUM}" id="lnkImgcom" style="margin-top: -15.5px;margin-left: 0px;" height="12" width="12"  url="{!$Resource.HUMHoverOverQuestionMark}"/> <!-- QS UI Redesign - included style -->
                                    </apex:outputPanel>
                                </td>
                                
                                <td align="right">
                                    <button type="button" id="btnReset"  class="btn btn-primary btn-sm pull-right" style="display:none;margin-top: 7px;" onClick="resetAll();">Reset</button>
                                </td>
                            </tr>
                        </table>   
                    </div> 
                </div> 
            </div> 
        </div> 
    </div>
    <div id="div-row-split" class="row">
    </div>
    <div id="div-state-popin" style="display:none;">
        <div id="div-popin-content" class="row">
            <p id="p-popin-firstmessage">Pop-Out the window to begin using {!sQSLabel}</p>
            <p id="p-popin-secondmessage">Click the arrow icon above</p><br></br>
            <apex:image id="img-popin-icon" styleClass="popin-icon" url="{!URLFOR($Resource.QuickStart_SR_AHT_HUM, 'QuickStart_SR_AHT_HUM/icons/QSImage.jpg')}" height="120" width="300"/>           
        </div>
    </div>
    <div id="div-state-popout" style="display:none;">
        <div id="div-container-cmp">
            <apex:form >
                <apex:actionFunction name="createCaseAndOpenAF" action="{!assignCaseValues}" onComplete="openNewCaseInSubtab(); return false;" rerender="outCaseJs,errorMessageid">
                    <apex:param id="sFocusedObjectIdValue" name="focusedObj" assignTo="{!sFocusedObjId}" value=""></apex:param>
                    <apex:param id="sinteractionIdValue" name="interactionId" assignTo="{!sInteractionId}" value=""></apex:param>
                    <apex:param id="cid" name="sClassificationSelect" assignTo="{!sClassificationSelect}" value=""></apex:param>
                    <apex:param id="intentID" name="sIntentSelect" assignTo="{!sIntentSelect}" value=""></apex:param>
                </apex:actionFunction>
                <apex:actionFunction name="resetAF" action="{!reset}"/> 
                <apex:actionFunction name="resetIntentAF" rerender="inDropIntent,caseCommentPanel,outPanelDocLinks"/>
                <apex:actionFunction name="complaintFieldDependenciesAF" rerender="inDropComplaintreason,inDropComplainttype"/>
                <apex:actionFunction name="gaFieldDependenciesAF" rerender="inDropGareason" oncomplete="disableInputFields();"/>
                <apex:actionFunction name="setClassificationAF" rerender="inDropIntent" oncomplete="activateIntent();hasUserInputOnPretextTemplateClassification();setGhostCallIntent();">
                    <apex:param id="classificationGCId" name="classificationGCName" assignTo="{!sClassificationSelect}" value=""></apex:param>
                </apex:actionFunction>
                <apex:actionFunction name="IntentAF" action="{!populateDocumentLinks}" rerender="outPanelDocLinks, caseCommentPanel,HPCaseOPId,recActionPanelId" oncomplete="resetGAComplaintPanel();hideDupGrievanceMessage();enableGA();findIsHPCase();showloggingMessage('{!sHeaderMessageType}');hideUpdateLoading();">
                    <apex:param id="intentID1" name="sIntentSelectValue" assignTo="{!sIntentSelect}" value=""></apex:param>
                </apex:actionFunction>
		<apex:actionFunction name="gaSectionRenderAF" action="{!getAFPretextGA}" oncomplete="createDIVGA('{!sPretextGAJSON}');">
                    <apex:param id="ogoFieldID1" name="sOGOFieldSelectValue" assignTo="{!sOGOFieldValue}" value=""></apex:param>
                    <apex:param id="ogoReasonID1" name="sOGOReasonSelectValue" assignTo="{!sOGOReasonValue}" value=""></apex:param>
                </apex:actionFunction>
                <apex:actionFunction name="dupComplaint" action="{!checkDuplicateGrievance}" rerender="duplicateGrievancesPanel,grievancePanel" oncomplete="initializeValues()">
                    <apex:param id="classi" name="sClassificationSelect" assignTo="{!sClassificationSelect}" value=""></apex:param>
                    <apex:param id="intenti" name="sIntentSelect" assignTo="{!sIntentSelect}" value=""></apex:param>
                    <apex:param id="accid" name="sAccountId" assignTo="{!sAccountId}" value=""></apex:param>
                </apex:actionFunction>
                <apex:actionFunction name="dupClassification" action="{!checkDuplicateClassification}" rerender="duplicateClassificationPanel,classificationCheckPanel" oncomplete="getShowClassificationInfo()">
                    <apex:param id="dup_classi" name="sClassificationSelect" assignTo="{!sClassificationSelect}" value=""></apex:param>
                    <apex:param id="accid_classi" name="sAccountId" assignTo="{!sAccountId}" value=""></apex:param>
                </apex:actionFunction>
                <apex:actionFunction name="fetchpolicy" action="{!fetchPolicyMember}" rerender="validationPanel" oncomplete="getValidationInfo()">
                    <apex:param id="intentpolicy" name="sIntentSelect" assignTo="{!sIntentSelect}" value=""></apex:param>
                </apex:actionFunction>
                
                <apex:actionFunction name="commentsAF" reRender="commentsBox" oncomplete="getPopupForCaseHistory('{!$CurrentPage.parameters.elemID}');">
                    <apex:param name="caseIdValues" value=""/>
                    <apex:param name="elemID" value=""/>
                </apex:actionFunction>
                
                <apex:outputPanel layout="block" id="commentsBox" >
                    <div id="PopUp3" class="arrow_box" style="display: none;">
                        <c:HUMPopUPForCaseComments id="CustomComponent" CaseId="{!$CurrentPage.parameters.caseIdValues}"/>
                    </div>
                </apex:outputPanel>

                <div id="div-panel-cmp" class="panel panel-default">
                    <div class="panel-body">
                        <div id="div-row-info" class="row">
                            <div id="div-alert-container">
                                <div id="div-sizealert-qs" class="alert alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="qs-SizeError" value="/img/msg_icons/error24.png"/>
                                    </div>
                                   <div id="qs-sizeErrormsg" class="div-alert-messagecontainer"></div>
                                </div> 
    
                                <div id="div-alert-qs" class="alert alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="qs-Error" value="/img/msg_icons/error24.png"/>
                                    </div>
                                    
                                    <div id="qs-Error-messagecontainer" class="div-alert-messagecontainer"> 
                                        <apex:pageMessages id="errorMessageid"/> 
                                    </div>
                                </div>
                                <div id="div-alert-multipolmem" class="alert alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="Warning" value="/img/msg_icons/warning24.png"/>
                                    </div> 
                                    <div class="div-alert-inputmessagecontainer">
                                        <b>Warning: </b><br/>{!$Label.QUICKSTART_MULTIPOLMEM_HUM}
                                        <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="createCaseAndOpen()">Yes</a>
                                        <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="hideAlerts()">No</a>
                                    </div>                              
                                </div>
                                
                                <!-- Multiple Case open warning message -->
                                
                                <div id="div-alert-multiCase" class="alert alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="Warning2" value="/img/msg_icons/warning24.png"/>
                                    </div> 
                                    <div class="div-alert-inputmessagecontainer">
                                        <b>Warning: </b><br/> {!$Label.QUICKSTART_MULTICASE_HUM}
                                        <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="updatecommentsToCase()">Yes</a>
                                        <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="hideAlerts()">No</a>
                                    </div>                              
                                </div>
                                                            
