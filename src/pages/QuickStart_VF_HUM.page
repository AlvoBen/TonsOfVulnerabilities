<apex:page controller="QuickStart_C_HUM" showheader="false" sidebar="false" standardstylesheets="false"> 
    <meta content="maximum-scale=1.0, initial-scale=1.0, width=device-width" name="viewport" />
    <apex:includescript value="/support/console/36.0/integration.js" /> 
    <apex:includescript value="{!URLFOR($Resource.CRM_JQUERY, 'jquery.min.js')}" /> 
    <apex:includescript value="{!URLFOR($Resource.CRM_DataTables_1_13_4, 'datatables.min.js')}" />
    <link rel="stylesheet" type="text/css" href="{!URLFOR($Resource.CRM_DataTables_1_13_4, 'datatables.min.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.CRM_PLATFORM_TOOLKIT, 'datatables_HUM.min.css')}" />
    <apex:stylesheet value="{!URLFOR($Resource.QuickStart_SR_HUM, 'QuickStart_SR_HUM/styles/QuickStart_VF_Style.css')}"/>
    <apex:includescript value="{!URLFOR($Resource.QuickStart_SR_HUM,'QuickStart_SR_HUM/scripts/QuickStart_VF_Script.js')}"/>

    <apex:stylesheet value="{!URLFOR($Resource.QuickStart_SR_HUM, 'QuickStart_SR_HUM/styles/QuickStart_CMP_Style.css')}"/>
    <apex:includescript value="{!URLFOR($Resource.QuickStart_SR_HUM,'QuickStart_SR_HUM/scripts/QuickStart_CMP_Script.js')}"/>
    <apex:includescript value="{!URLFOR($Resource.QuickStart_SR_HUM,'QuickStart_SR_HUM/scripts/QuickStart_CMP_Pretext_Script.js')}"/>
	<apex:includeScript value="{!URLFOR($Resource.CRM_JQUERY,'bootstrap-datepicker-uni.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.DatePickerOverrides_SR_HUM, '/js/datePickerOverrides.js')}"/> 
    <link rel="stylesheet" text="text/css" href="{!URLFOR($Resource.CRM_JQUERY, 'bootstrap-datepicker-uni.css')}" /> 
    <apex:stylesheet value="{!URLFOR($Resource.DatePickerOverrides_SR_HUM,'/css/datePickerOverrides.css')}"/>

    <div class="row">
        <div id="div-container-page">
            <div id="div-col-header" class="col-xs-12">
                <div id="div-panel-header" class="panel panel-default"> 
                    <div class="panel-heading">
                    	<table align="center" width="100%" border="0">
                        	<tr>
                                <td align="left"><h3 class="panel-title">{!sQSLabel}</h3></td>
                        	    <td align="right">
                                 <apex:outputPanel id="pretextdisabledPanel" rendered="{!NOT(bPreTextConfigured)}">
                                        <h3 class="panel-title" style="margin-right: 15px;">Pretext: Disabled</h3>
                                        <apex:image title="{!$Label.QUICKSTART_PRETEXTCONFIGURE_HUM}" id="lnkImgcom" style="margin-top: -15.5px;margin-left: 0px;" height="12" width="12"  url="{!$Resource.HUMHoverOverQuestionMark}"/>
                                  </apex:outputPanel>
								</td>
                        	</tr>
                        </table>   
                    </div> 
                </div> 
            </div> 
        </div> 
    </div>
    <div id="div-row-split" class="row">
    </div>
    <div id="div-state-popin" style="display:none;">
        <div id="div-popin-content" class="row" style= "margin-top: 165px; !important">
            <p id="p-popin-firstmessage">Pop-Out the window to begin using {!sQSLabel}</p>
            <p id="p-popin-secondmessage">Click the arrow icon above</p><br></br>
            <apex:image id="img-popin-icon" styleClass="popin-icon" url="{!URLFOR($Resource.QuickStart_SR_HUM, 'QuickStart_SR_HUM/icons/QSImage.jpg')}" height="120" width="300"/>           
        </div>
    </div>
    <div id="div-state-popout" style="display:none;">
        <div id="div-container-cmp">
            <apex:form >
                <apex:actionFunction name="createCaseAndOpenAF" action="{!assignCaseValues}" onComplete="openNewCaseInSubtab(); return false;" rerender="outCaseJs,errorMessageid">
                    <apex:param id="sFocusedObjectIdValue" name="focusedObj" assignTo="{!sFocusedObjId}" value=""></apex:param>
                    <apex:param id="sinteractionIdValue" name="interactionId" assignTo="{!sInteractionId}" value=""></apex:param>
                    <apex:param id="cid" name="sClassificationSelect" assignTo="{!sClassificationSelect}" value=""></apex:param>
                    <apex:param id="intentID" name="sIntentSelect" assignTo="{!sIntentSelect}" value=""></apex:param>
                </apex:actionFunction>
                <apex:actionFunction name="resetAF" action="{!reset}"/> 
                <apex:actionFunction name="resetIntentAF" rerender="inDropIntent,caseCommentPanel,outPanelDocLinks"/>
                <apex:actionFunction name="complaintFieldDependenciesAF" rerender="Complaintreason,inDropComplainttype"/>
                <apex:actionFunction name="gaFieldDependenciesAF" rerender="inDropGareason" oncomplete="disableInputFields();"/>
                <apex:actionFunction name="setClassificationAF" rerender="inDropIntent" oncomplete="activateIntent();hasUserInputOnPretextTemplateClassification();setGhostCallIntent();">
                    <apex:param id="classificationGCId" name="classificationGCName" assignTo="{!sClassificationSelect}" value=""></apex:param>
                </apex:actionFunction>
                <apex:actionFunction name="IntentAF" action="{!populateDocumentLinks}" rerender="outPanelDocLinks, caseCommentPanel,HPCaseOPId,recActionPanelId" oncomplete="resetGAComplaintPanel();hideDupGrievanceMessage();enableGA();findIsHPCase();showloggingMessage();hideUpdateLoading();"> 
                    <apex:param id="intentID1" name="sIntentSelectValue" assignTo="{!sIntentSelect}" value=""></apex:param>
                </apex:actionFunction>
                <apex:actionFunction name="allowDupClassificationAF" rerender="none" oncomplete="complaintFieldDependenciesAF(); gaFieldDependenciesAF();">
                    <apex:param id="duplicateClassification" name="duplicateClassification" assignTo="{!bErrorDuplicClassification}" value=""></apex:param>
                </apex:actionFunction>
                <apex:actionFunction name="gaSectionRenderAF" action="{!getAFPretextGA}"  reRender="grievanceAppealPanel" oncomplete="displayGApretext();">
                    <apex:param id="ogoFieldID1" name="sOGOFieldSelectValue" assignTo="{!sOGOFieldValue}" value=""></apex:param>
                    <apex:param id="ogoReasonID1" name="sOGOReasonSelectValue" assignTo="{!sOGOReasonValue}" value=""></apex:param>
                </apex:actionFunction>
                <apex:actionFunction name="dupComplaint" action="{!checkDuplicateGrievance}" rerender="duplicateGrievancesPanel,grievancePanel" oncomplete="initializeValues()">
                    <apex:param id="classi" name="sClassificationSelect" assignTo="{!sClassificationSelect}" value=""></apex:param>
                    <apex:param id="intenti" name="sIntentSelect" assignTo="{!sIntentSelect}" value=""></apex:param>
                    <apex:param id="accid" name="sAccountId" assignTo="{!sAccountId}" value=""></apex:param>
                </apex:actionFunction>
                <apex:actionFunction name="commentsAF" reRender="commentsBox" oncomplete="getPopupForCaseHistory('{!$CurrentPage.parameters.caseIdValues}','{!$CurrentPage.parameters.elemID}');">
                    <apex:param name="caseIdValues" value=""/>
                    <apex:param name="elemID" value=""/>
                </apex:actionFunction>
                
                <apex:outputPanel layout="block" id="commentsBox" >
                    <div id="PopUp3" class="arrow_box" style="display: none;">
                        <c:HUMPopUPForCaseComments id="CustomComponent" CaseId="{!$CurrentPage.parameters.caseIdValues}"/>
                    </div>
                </apex:outputPanel>
                <div id="div-panel-cmp" class="panel panel-default">
                    <div class="panel-body">
                        <div id="div-row-info" class="row">
                            <div id="div-alert-container">
                                <div id="div-sizealert-qs" class="alert alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="qs-SizeError" value="/img/msg_icons/error24.png"/>
                                    </div>
                                    <div id="qs-sizeErrormsg" class="div-alert-messagecontainer"></div>
                                </div> 
                                    <div id="div-alert-qs" class="alert alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="qs-Error" value="/img/msg_icons/error24.png"/>
                                    </div>
                                    <div id="qs-Error-messagecontainer" class="div-alert-messagecontainer"></div>
                                    <div id="qs-Pages-ErrorMessage" class="div-alert-messagecontainer">
                                        <apex:pageMessages id="errorMessageid"/> 
                                    </div>
                                </div>
                                <div id="div-alert-multipolmem" class="alert alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="Warning" value="/img/msg_icons/warning24.png"/>
                                    </div> 
                                    <div class="div-alert-inputmessagecontainer">
                                        <b>Warning: </b><br/>{!$Label.QUICKSTART_MULTIPOLMEM_HUM}
                                           <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="createCaseAndOpen()">Yes</a>
                                           <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="hideAlerts()">No</a>
                                    </div>                              
                                </div>
                                <div id="div-alert-multiCase" class="alert alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="Warning2" value="/img/msg_icons/warning24.png"/>
                                    </div> 
                                    <div class="div-alert-inputmessagecontainer">
                                        <b>Warning: </b><br/> {!$Label.QUICKSTART_MULTICASE_HUM}
                                           <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="updatecommentsToCase()">Yes</a>
                                           <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="hideAlerts()">No</a>
                                    </div>                              
                                </div>
                                                            
                                <div id="div-alert-pretexttemplate" class="alert alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="Warning-pretext" value="/img/msg_icons/warning24.png"/>
                                    </div> 
                                    <div class="div-alert-inputmessagecontainer">
                                        <b>Warning: </b><br/>{!$Label.QUICKSTART_PRETEXTTEMPLATE_HUM}
                                           <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="resetPretextTemplate()">Yes</a>
                                           <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="hideAlerts();defaultPretextTemplate();">No</a>
                                    </div>                              
                                </div>
                                <div id="div-alert-WarningHeaderMessage-show" class="alert alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="WarningHeaderMessage-show-image" value="/img/msg_icons/warning24.png"/>
                                    </div>
                                    <div class="div-alert-inputmessagecontainer">
                                        <b>Warning: </b><br/>
                                        <div id="div-alert-WarningHeaderMessage"></div>
                                        <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="yesAutoLog();">Yes</a>
                                        <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="noAutoLog();">No</a>
                                    </div>
                                </div>
                                <div id="div-alert-InfoHeaderMessage-show" class="alert" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="InfoHeaderMessage-show-image" value="/img/msg_icons/info24.png" />
                                    </div>
                                    <div id="info-autoLogging" class="div-alert-messagecontainer">
                                        <b>Info: </b>
                                        <div id="div-alert-InfoHeaderMessage"></div>
                                    </div>
                                </div>
                               <div id="div-alert-existing-grievance" class="alertGuidance alert-warning" style="display: none;" role="alert">
                                   <div class="div-alert-iconcontainer" style="vertical-align:top">
                                       <apex:image id="Warning-existing-grievance" value="/img/msg_icons/info24.png" />
                                    </div>
                                    <div class="div-alert-inputmessagecontainer">
                                        <b>Info: </b><br/>{!$Label.QUICKSTART_DUPLICATE_GRIEVANCE_HUM}&nbsp;&nbsp;
                                        <apex:outputPanel id="duplicateGrievancesPanel">
                                            <apex:repeat id="rptDocLinknum" value="{!listOfClassObj}" var="oLink" >
                                                <a href="#" class="linkDocuments" id="case-link-{!oLink.Id}" style="margin-left: 0px; !important" onclick="openCaseInSubTab('{!oLink.Id}','{!oLink.Title}')" onmouseover="showCaseComment('{!oLink.Id}',this.id);" onmouseout="hidecasecomment();">{!oLink.Title}</a>
                                            </apex:repeat>
                                        </apex:outputPanel>
                                    </div>
                                </div>
                                <div id="div-alert-existing-classification" class="alert alert-warning" style="display: none;" role="alert">
                                    <div class="div-alert-iconcontainer" style="vertical-align:top">
                                        <apex:image id="Warning-existing-classification" value="/img/msg_icons/warning24.png"/>
                                    </div>
                                    <div class="div-alert-inputmessagecontainer">
                                        <b>Warning: </b><br/>{!$Label.QUICKSTART_DUPLICATECLASS_L14_HUM}
                                        <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="yesDuplicateClassification();">Yes</a>
                                        <a href="#" class="btn btn-default btn-xs alert-link btnstyle" onclick="noDuplicateClassification();">No</a>
                                    </div>
                                </div>
                            </div>
                            
                            <div id="div-alert-outstandingitem" class="alertGuidance alert-warning" style="display: none;" role="alert"> 
                                <div class="div-alert-iconcontainer" style="vertical-align:top"> 
                                    <apex:image id="outstandingitem-Error" value="/img/msg_icons/info24.png" /> 
                                </div> 
                                <div id="verify-outstandingitem" class="div-alert-messagecontainer"></div> 
                            </div> 
                            <div>
                                <apex:outputPanel id="HPCaseOPId">
                                    <input type="hidden" id="isHPCaseId" value="{!bIsHPCase}"/>
                                </apex:outputPanel>
                                <button type="button" id="btnReset" class="btn btn-primary btn-xs pull-right" onClick="resetAll();">Reset</button>                                
                                
                            </div>
                        </div> 
                        <div class="row">
                            <div id="div-col-left" class="col-sm-4 col-xs-4"> 
                                <div class="div-padding">
                                    <div class="row"> 
                                        <div class="panel panel-default"> 
                                            <div class="panel-heading">Case Information</div>
                                            <div class="panel-body">
                                                <div class="col-xs-12 div-fieldgroup"> 
                                                    <div class="form-group">
                                                        <label id="lblClassification" for="inDropClassification">Classification</label>
                                                        <apex:actionRegion >
                                                            <apex:selectList id="inDropClassification" styleClass="form-control input-required" value="{!sClassificationSelect}" size="1" required="false">
                                                                <apex:selectOptions id="outDropOptnClassification" value="{!lstClassificationOptions}"/>
                                                                <apex:actionSupport event="onchange" rerender="inDropIntent,recActionPanelId" oncomplete="setFrequentlyUsedCI();resetGAComplaintPanel();hideDupGrievanceMessage();"/> 
                                                            </apex:selectList>
                                                        </apex:actionRegion>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 div-fieldgroup"> 
                                                    <div class="form-group"> 
                                                        <label id="lblIntent" for="inDropClassification">Intent</label>
                                                        <apex:actionRegion >
                                                            <apex:selectList id="inDropIntent" styleClass="form-control input-required" value="{!sIntentSelect}" disabled="false" size="1" required="false" onchange="checkAndsetIntentJS(); resetGhostCall(); return false;">
                                                                <apex:selectOptions id="outDropOptnIntent" value="{!lstIntentOptions}" />
                                                            </apex:selectList>
                                                        </apex:actionRegion>
                                                    </div>
                                                </div>
                                                <apex:outputPanel id="outPanelMedicareCallPartCOrPartD" rendered="{!$Permission.RSOMedicareFrontOfficeUser_HUM}">
                                                     <div class="col-xs-12 div-fieldgroup"> 
                                                        <div class="form-group"> 
                                                            <label id="lblMedicareCallPartCOrPartD" for="inDropClassification">Medicare Call Part C or Part D</label>
                                                            <apex:inputField id="inpMedicareCallPartCOrPartD" styleClass="form-control input-required" value="{!oNewCase.Call_Benefit_Category__c}" onchange="setComplaint();"/>
                                                        </div>
                                                    </div> 
                                                 </apex:outputPanel>                                             
                                            </div> 
                                        </div>
                                        <apex:outputPanel id="recActionPanelId">
                                            <apex:outputPanel rendered="{!bDisplayRecommendedActionsIcon}">
                                                <div class="panel panel-default">
                                                    <div class="panel-heading">Recommended Actions</div>
                                                    <div class="panel-body">
                                                        <div class="col-xs-12 div-fieldgroup">
                                                            <apex:repeat value="{!lstqspRecActions}" var="docName">
                                                                 <apex:image title="{!mapRAiconImages[docName][1]}" value="{!'/servlet/servlet.FileDownload?file='+mapRAiconImages[docName][0]}" height="40px" width="40px"/>&nbsp;&nbsp;&nbsp;&nbsp;
                                                            </apex:repeat>
                                                        </div>
                                                    </div>
                                                </div>
                                             </apex:outputPanel>
                                        </apex:outputPanel>  
                                         
                                        <div class="panel panel-default">
                                            <div class="panel-heading">G&amp;A/Complaints</div>
                                            <div class="panel-body">
                                                <div class="col-xs-12 div-fieldgroup" >
                                                    <div class="form-group">
                                                        <input id="inChkOnGARGC" name="CheckedOnGARGC" type="checkbox" onclick="setGARGCFields(1);hasUserInputOnPretextTemplateGAComplaint();resetGAComplaintPanel();hideDupGrievanceMessage();"></input>
