    <link rel="stylesheet" href="{!URLFOR($Resource.GroupIdCards_SR_HUM, 'GroupIdCards_CSS_HUM.css')}" />
    <link rel="stylesheet" type="text/css" href="{!URLFOR($Resource.CRM_PLATFORM_TOOLKIT, 'datatables_HUM.min.css')}" />

    <script type="text/javascript">
//dont enable P9 Critical Fix    var $jIdCard = jQuery.noConflict();//Please do not change this line as it breaks the JS tables.
    </script>

    <div class="width100">
      <div id="loggingStickyHeader" class="LoggingCompCSS width100">
        <c:Logging_CMP_HUM DisplayMessage="false" Logging_Color="red" AttachmentKey="{!objAcc.Account_External_ID__c}" AttachmentDesc="Account_External_ID" Logged_Item_Color="blue" Screenshot_Area_Id="pageContent" RecordId="{!sAccId}" />
      </div>
      <div id="pageContent" class="pgcon">
            <div class="sectionheader">
                <apex:sectionheader title="{!$Label.IDCARDMGTSECTIONHEADER_GROUPIDCARDS_HUM}" subtitle="{!objAcc.Name}" />
            </div>
            <apex:outputPanel id="stdMap">
                <input type="hidden" id="StandardMapValuesForLogging" value="" />
            </apex:outputPanel>
            <apex:pageBlock mode="maindetail">
                
                <apex:pageBlockSection title="{!$Label.CARDSTATUSSECTION_GROUPIDCARDS_HUM}" collapsible="true" columns="1" id="thePageBlockSectionCreditCardInfo">
                    <apex:outputpanel >                        
                        <div id="infoMessage">
                            <apex:PageMessage rendered="true" strength="2" summary="{!$Label.INFOMESSAGE_GROUPIDCARDS_HUM}" severity="Info" />
                        </div>
                        <div id="noRecordsMessage" style="display:none;">
                            <apex:PageMessage strength="2" summary="{!$Label.NORECORDSMESSAGE_GROUPIDCARDS_HUM}" severity="Info" />
                        </div>
                        <div id="errorLogMessage" style="display:none;">
                            <apex:PageMessage strength="2" summary="{!$Label.ERRORLOGMESSAGE_GROUPIDCARDS_HUM}" severity="error" />
                        </div>
                        <c:BaseDataTable_CMP_HUM jqueryVariable="jQuery"
                                                 tableid="groupIdCardTable"
                                                 tablecontainerid="groupIdCardTableContainer"
                                                 enablesearch="f"
                                                 info="true"
                                                 keyword="false"
                                                 sort="false"
                                                 columnname="{!columnName}"
                                                 htmlColumnName="sCardRequestDate"
                                                 objName="Account"
                                                 col1_accordian="false"
                                                 col1_editDelete="false"
                                                 col1_orderable="true"
                                                 columnHeader="{!columnHeader}"
                                                 orderColumn="0"
                                                 orderDirection="desc"
                                                 orderingBool="false" 
                                                 />
                    </apex:outputpanel>
                </apex:pageBlockSection>
            </apex:pageBlock>
        </div>
    </div>
    <script type="text/javascript">
      var vGroupNumber = '{!JSENCODE(objAcc.Source_Customer_Key__c)}';
      var vGroupName = '{!JSENCODE(objAcc.Name)}';
      var vGroupId = '{!JSENCODE(objAcc.Id)}';
      var recordId = '{!$CurrentPage.parameters.Id}';
      var vGroupIdCardAction = '{!$RemoteAction.GroupIdCards_C_HUM.invokeGrpService}';
    </script>
</apex:page>