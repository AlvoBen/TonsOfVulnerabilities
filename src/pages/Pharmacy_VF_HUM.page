  <!-- Moved the change from dividing the page into 1 table, to making 2 div tags and added the collapsiblesidepanel component -->
  <div id="leftPanel" style="width:16%; display:inline-flex;">
    <div id="column1" class="Links" >
      <c:DynamicCustomLinks_CMP_HUM DynamicLinkController="{!this}"/>
    </div>
    <div id="collapsePanel">
      <c:CollapsibleSidePanel_CMP_HUM leftPanelID="leftPanel" leftCMPID="column1" rightPanelID="rightPanel" collapsePanelID="collapsePanel" jqueryVariable="jQuery"/>
    </div>
  </div>

  <apex:outputPanel >
    <div id="rightPanel">
      <div id="pharmacySection" class="pgcon">
        <apex:outputPanel styleClass="floatingLinks,fixedDiv sticky" id="floatLinkId">
          <c:PharmacyFloatingLinks_CMP_HUM id="flotingLinksCompId" memberID="{!objPolMem.name}" bShowMTMSection="{!bShowMTMSection}"/>
        </apex:outputPanel>
		<apex:outputPanel id="pcoveo" rendered="{!bCoveoUser}">
			<div style="padding: 34px 0px 0px!important;margin-top: 20px;margin-bottom:-23px !important" >
				<c:UnifiedSearch_CMP_HUM id="uniId" policyMemberId="{!objPolMem.Id}" PageNameToPass="Humana Pharmacy" />
			</div>		
		</apex:outputPanel>
        <div id="PharmacyPageHeader" style="padding: 15px 0px 0px!important;">
          <apex:sectionheader title="{!$Label.HUMPharmacyTitle}" subtitle="{!objPolMem.name}"/>
        </div>

        <apex:form id="creditCardIcon">
            <apex:actionFunction name="ProcessCreditCard" reRender="pnlIcons" oncomplete="getMemberIconStatus();" >
              <apex:param name="ExpiredCard" assignTo="{!bExpiredCreditCard}" value="" />
              <apex:param name="ExpiringSoonCard" assignTo="{!bExpiringSoonCreditCard}" value="" />
			  <apex:param name="Authorization" assignTo="{!bPharmacyAuth}" value="" />
            </apex:actionFunction>
              <c:MemberIcons_CMP_HUM RecordId="{!objPolMem.id}" Expired="{!bExpiredCreditCard}" ExpiringSoon="{!bExpiringSoonCreditCard}" Authorization="{!bPharmacyAuth}" ParentPageName="Humana Pharmacy"></c:MemberIcons_CMP_HUM> 
        </apex:form>
        
        <apex:form >
          <apex:pageBlock mode="maindetail" id="pbPharmacy">
            <!-- humana pharmacy to show member not found message -->
            <apex:outputPanel id="errorMsgMemberNotFoundPanel"/>
            <!-- For Constructor Related exception -->
            <c:HUMConstructorExceptionComponent errorLogs="{!lstLogErrors}" />

            <!-- humana pharmacy off line mode message -->
            <apex:outputPanel id="offlineModeMsgId" Styleclass="displayNone">
              <apex:PageMessage Summary="{!$Label.HUMPharmacyOfflineModeMsg}" severity="INFO" Strength="3" />
              <apex:pagemessages />
            </apex:outputPanel>

            <!-- humana pharmacy Demographics Section -->
            <a id="linkPharmacyDemographicDetails" name="Demographics"></a>
            <apex:pageBlockSection collapsible="true" columns="1" title="{!$Label.Pharmacy_Demographics}" id="thePageBlockSectionDemographics">
              <c:PharmacyDemographic_CMP_HUM id="pharmacyDemographicDetails" />
            </apex:pageBlockSection>

            <!-- humana pharmacy Medication Therapy Management Section -->
            <a id="linkPharmacyMtmEligibility" name="Medication Therapy Management"></a>
            <apex:pageBlockSection rendered="{!bShowMTMSection}" collapsible="true" columns="1" title="Medication Therapy Management (MTM) Eligibility" id="thePageBlockSectionMTM">
              <c:PharmacyMTMEligibility_CMP_HUM id="pharmacyMtmEligibility" />
            </apex:pageBlockSection>

            <!-- humana pharmacy Addresses Section -->
            <a id="linkPharmacyAddressComp" name="Addresses"></a>
            <apex:pageBlockSection collapsible="true" columns="1" title="Addresses" id="thePageBlockAddress">
              <apex:outputPanel id="classicAddress" rendered="{!NOT(bSwitchLightningAddress)}">
                <c:PharmacyViewAddress_CMP_HUM id="pharmacyAddressComp" />
              </apex:outputPanel>
              <apex:outputPanel id="lightningAddress" rendered="{!bSwitchLightningAddress}">
                <apex:includeLightning />
              
                <div id="LightningAddressDivId" style="max-width: 500px; zoom:1.3;"></div>
              
                <script>       
					let interactionId='';
					var primaryTabPageId = function primaryTabPageId(result) {
						primaryAccountTabID = result.id;
						sforce.console.getPageInfo(primaryAccountTabID , showPrimaryPageInfo);
					};

					sforce.console.getEnclosingPrimaryTabId(primaryTabPageId);
					var showPrimaryPageInfo = function (result) {
						var obj = JSON.parse(result.pageInfo);
						if(obj.url.includes("&interactionId=")){
							interactionId = obj.url.split("&interactionId=")[1].substring(0,18);
												
							$Lightning.use(
							"c:PharmacyAddress_LCMP_HUM",
								function () {
								  $Lightning.createComponent(
									"c:pharmacyAddressHum",
									{ "enterpriseId": enterpriseID, "networkId": netWorkId, "recordId": recordId, "policyId": policyId, "interactionId": interactionId},
									"LightningAddressDivId"
								  );
								}
							);
						} else{
							$Lightning.use(
							"c:PharmacyAddress_LCMP_HUM",
								function () {
								  $Lightning.createComponent(
									"c:pharmacyAddressHum",
									{ "enterpriseId": enterpriseID, "networkId": netWorkId, "recordId": recordId, "policyId": policyId},
									"LightningAddressDivId"
								  );
								}
							);
						} 
					};
				</script>
              </apex:outputPanel>
            </apex:pageBlockSection>

            <!-- humana pharmacy Case History Section -->
            <a id="linkPharmacyCaseHistory" name="CaseHistory"></a>
            <apex:pageblocksection id="thePageBlockCaseHis" title="Case History" collapsible="true" columns="1"  >
              <c:PharmacyCaseHistory_CMP_RT_HUM id="pharmacyCaseHistory" />
            </apex:pageblocksection>

            <!-- humana pharmacy OMSNotes Section -->
            <a id="linkPharmacyOMSNotesDetails" name="OMSNotes"></a>
            <apex:pageBlockSection collapsible="true" columns="1" title="{!$Label.Pharmacy_OMS_Originated_Notes}" id="thePageBlockSectionOMSNotes">
              <c:Pharmacy_OMS_Originated_Notes_CMP_HUM id="pharmacyOMSNotesDetails" />
            </apex:pageBlockSection>

            <!-- humana pharmacy Order Summary Section -->
            <a id="linkPharmacyOrderSummaryComp" name="OrderSummary"></a>
            <apex:pageBlockSection collapsible="true" columns="1" title="Order Summary" id="thePageBlockSectionOrderSummary">
              <c:PharmacyOrderSummary_CMP_HUM id="pharmacyOrderSummaryComp"  />
            </apex:pageBlockSection>

            <!-- humana pharmacy Member Prescription Section -->
            <a id="linkPharmacyMemberPrescriptionComp" name="Member Prescription"></a>
            <apex:pageBlockSection collapsible="true" columns="1" title="Member's Prescriptions" id="thePageBlockSectionMemberPharmacy">
              <c:PharmacyMemberPrescription_CMP_HUM id="pharmacyMemberPrescriptionComp"  />
            </apex:pageBlockSection>

            <!-- humana pharmacy Health Conditions and Allergies Section -->
            <a id="linkPharmacyAllergyComp" name="Health Conditions and Allergies"></a>
            <apex:pageBlockSection collapsible="true" columns="1" title="{!$Label.Health_Con_and_Allergies}" id="thePageBlockSectionAllergies">
              <c:PharmacyAllergyAndHcondition_CMP_HUM id="pharmacyAllergyComp"  />
            </apex:pageBlockSection>
            <apex:outputPanel id="futureDate">
              <input type="hidden" id="futureDatedID"/>
            </apex:outputPanel>
          </apex:pageBlock>
        </apex:form>
      </div>
    </div>
  </apex:outputPanel>
 
