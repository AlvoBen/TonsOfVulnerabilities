<apex:page id="InactivateRxRequestPage" controller="PharmacyInactivateRx_C_HUM">
    <apex:includeScript value="{!URLFOR($Resource.CRM_JQUERY, 'jquery.min.js')}" />
    <apex:includeScript value="/support/console/54.0/integration.js" />
    <apex:includescript value="{!URLFOR($Resource.CRM_JQUERY, 'jquery.js')}" />
    <link rel="stylesheet" text="text/css" href="{!URLFOR($Resource.PharmacyInactivateRxToCase_SR_HUM, 'PharmacyInactivateRxToCase_CSS_HUM.css')}"/>
    <apex:includeScript value="{!URLFOR($Resource.PharmacyInactivateRxToCase_SR_HUM,'PharmacyInactivateRxToCase_JS_HUM.js')}"/>
    
    <apex:outputpanel >
        <br/>
        <font class="headerTitle">
            <b>{!$Label.InactivateRxTitle}</b>
        </font>
        <br/>
        
        <flow:interview id="Inactivate_Rx" name="Inactivate_Rx" rendered="{!!hasProcess}" interview="{!inactivateRxFlow}" finishLocation="{!submissionvariable}">
            <apex:param name="Inactivate_Rx" value="{!InactivateRx}" />
            <apex:param name="TN" value="{!TN}" />
            <apex:param name="ParamInputSID" value="{!SubmissionID}" />
        </flow:interview>

        <flow:interview id="Inactivate_Rx_Overview" name="Inactivate_Rx" rendered="{!hasProcess}" interview="{!inactivateRxFlow}" finishLocation="{!$Page.HUMCloseFlow}">
                <apex:param name="ParamInputSID" value="{!SubmissionID}" />
                <apex:param name="InactivteRx_Overview_Data" value="{!SubmissionData}" />
                <apex:param name="TN" value="{!TN}" />
        </flow:interview>
        
    </apex:outputpanel>
    
    <script type="text/javascript">
        var hasProcess = {!JSENCODE(IF(hasProcess, 'true', 'false'))};
        if (hasProcess) {
            inactvateRxRemoveUnsavedChanges();
        }
    </script>
</apex:page>