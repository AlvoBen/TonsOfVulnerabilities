<apex:page id="InvoiceRequestPage" controller="InvoiceRequest_C_HUM">
    <apex:includeScript value="{!URLFOR($Resource.CRM_JQUERY, 'jquery.min.js')}" />
    <apex:includeScript value="/support/console/46.0/integration.js" />
    <apex:includescript value="{!URLFOR($Resource.CRM_JQUERY, 'jquery.js')}" />
    <apex:outputPanel rendered="{!!hasProcess}">
        <apex:includeScript value="{!URLFOR($Resource.PharmacyInvoiceRequestToCase_SR_HUM,'/PharmacyInvoiceRequestToCase_JS_HUM.js')}"/>
    </apex:outputPanel>
    
    <apex:outputpanel >
        <style type="text/css">
            .FlowPageBlockBtns {
                text-align: left !important;
            }

            .headerTitle {
                font-size: 16px;
                font-weight: bold;
            }

            label{
                font-size: 14px;
            }

            p {
                margin-left: 9%;
            }

            hr{
                margin-left: 3%;
                margin-right: 3%;
            }

            .customErrorMsg{
                margin-left: 10%;
            }

            .errorMsg p {
                margin-left: 0%;
            }

            .noteMsg{
                width: 9%;
                margin-left: 8.5%;
                text-align: right;
                margin-top: -75px;
            }

            .emailNote{
                width: 10%;
                margin-left: 9%;
                margin-top: -30px;
            }

            .noteYear{
                margin-top: -25px;
                text-align: right;
                width: 9%;
                margin-left: 9%;
            }

            .prefilledValues{
                text-indent: 50px;
                text-align: right;
                margin-right: 35%;
            }
        </style>

        <br/>
        <font class="headerTitle">
            <b>Invoice Request</b>
        </font>
        <br/>

        <flow:interview id="Invoice_Request" name="Invoice_Request" rendered="{!!hasProcess}" interview="{!invoiceFlow}" finishLocation="{!submissionvariable}">
            <apex:param name="Email_Input_Var" value="{!EmailAddress}" />
            <apex:param name="Billing_1" value="{!BillAddress}" />
            <apex:param name="Permanent" value="{!PermAddress}" />
            <apex:param name="Shipping" value="{!ShipAddress}" />
            <apex:param name="Alternate" value="{!AltAddress}" />
            <apex:param name="ParamInputSID" value="{!SubmissionID}" />
            <apex:param name="Invoice_Summary_Data" value="{!SubmissionData}" />
            <apex:param name="TN" value="{!TN}" />
        </flow:interview>

        <flow:interview id="Invoice_Request_Overview" name="Invoice_Request" rendered="{!hasProcess}" interview="{!invoiceFlow}" finishLocation="{!$Page.HUMCloseFlow}">
                <apex:param name="ParamInputSID" value="{!SubmissionID}" />
                <apex:param name="Invoice_Summary_Data" value="{!SubmissionData}" />
                <apex:param name="TN" value="{!TN}" />
        </flow:interview>
    </apex:outputpanel>

</apex:page>