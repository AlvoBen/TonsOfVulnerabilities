<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId,flexipage:availableForAllPageTypes,force:lightningQuickActionWithoutHeader" access="global" >
    <aura:attribute name="recordId" type="String" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <!-- this attribute is used to perform case comment manipulation on clicking on reassign case to creator-->
     <aura:attribute name="iRTC" type="Integer" default="0"/>
    <div>
    <c:caseTransferContainerHum recordId="{!v.recordId}" bReassignToCreator="true" oncloseaction="{!c.closeQuickAction}"></c:caseTransferContainerHum>
</div>
</aura:component>