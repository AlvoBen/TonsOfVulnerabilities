<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global">
    <lightning:navigationItemAPI aura:id="navigationItem" />
    <lightning:workspaceAPI aura:id="workspace" />
    <lightning:utilityBarAPI aura:id="utilityBar" />
    <aura:attribute name="url" type="String" />
    <aura:attribute name="tabname" type="String" />
    <lightning:messageChannel type="connectorHUM__c" onMessage="{!c.handleChanged}" scope="APPLICATION" />
    <lightning:messageChannel type="closeSubTabHUM__c" onMessage="{!c.handleClose}" scope="APPLICATION" />
    <aura:attribute name="pageReference" type="Object"/>
    <lightning:navigation aura:id="navService"/>
</aura:component>