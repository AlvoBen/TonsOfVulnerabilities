<aura:component
    implements="flexipage:availableForRecordHome,force:hasRecordId,lightning:actionOverride,lightning:isUrlAddressable,flexipage:availableForAllPageTypes,force:lightningQuickAction"
    access="global">
    <aura:attribute name="recordId" type="String" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />

    <div>
        <c:caseInformationComponentHum recordId="{!v.recordId}" encodedData="{!v.recordId}">
        </c:caseInformationComponentHum>
    </div>
</aura:component>