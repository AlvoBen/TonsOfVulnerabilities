<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId,flexipage:availableForAllPageTypes,force:lightningQuickActionWithoutHeader" access="global" >
    <aura:attribute name="recordId" type="String" />

    <div>
        <c:caseTransferContainerHum recordId="{!v.recordId}" bReassignToSender="true" oncloseaction="{!c.closeQuickAction}"></c:caseTransferContainerHum>
    </div>
</aura:component>