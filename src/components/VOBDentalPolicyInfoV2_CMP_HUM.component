                             <span id="priorCoverageIndicator">
                             </span>                                                            
                    </apex:pageBlockSectionItem>
                    <apex:pageBlockSectionItem id="DentalPolInfoPBSI2">
                        <apex:outputpanel >                            
                             <apex:outputLabel value="{!$Label.END_DATE}" style="font-size:12px;"/>
