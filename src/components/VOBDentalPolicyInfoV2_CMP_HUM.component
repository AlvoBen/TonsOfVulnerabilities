                             <apex:outputLabel value="{!$Label.PRIOR_ORTHO_INDICATOR}" style="font-size:12px;"/>
                        </apex:outputpanel>             
                             <span id="PriorOrthoIndicator">
                             </span>                                                            
                    </apex:pageBlockSectionItem>
                    <apex:pageBlockSectionItem id="DentalPolInfoPBSI5">
                        <apex:outputpanel >                            
                             <apex:outputLabel value="{!$Label.EFFECTIVE_DATE}" style="font-size:12px;"/>
                        </apex:outputpanel>             
                             <span id="EffectiveDate">
                             </span>                                                             
                    </apex:pageBlockSectionItem>
                    <apex:pageBlockSectionItem id="DentalPolInfoPBSI6">
                        <apex:outputpanel >                            
                             <apex:outputLabel value="{!$Label.INCOMPLETE_INDICATOR}" style="font-size:12px;"/>
                        </apex:outputpanel>               
                             <span id="IncompleteIndicator">
                             </span>                                                            
                    </apex:pageBlockSectionItem>
                    <apex:pageBlockSectionItem id="EnrollInfoHeaderPBSI">
                        <apex:outputLabel value="{!$Label.ENROLL_INFO}" styleClass="GroupHeaderStyle"/>
                    </apex:pageBlockSectionItem>
                    <apex:pageBlockSectionItem />
