<apex:component id="comp">
    <apex:attribute id="personID" name="personID" description="This is Person's ID" type="String" required="true"></apex:attribute>
    <apex:includeScript value="{!URLFOR($Resource.VOBDentalPolicyInfo_SR_HUM,'VOBDentalPolicyInfo_SR_HUM/VOBDentalPolicyInfo_JS_SR_HUM.js')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.VOBDentalPolicyInfo_SR_HUM, 'VOBDentalPolicyInfo_SR_HUM/VOBDentalPolicyInfo_CSS_SR_HUM.css')}"/>
    
    <script type="text/javascript">
    Visualforce.remoting.timeout = "{!$Label.HUMMemWebActivityTimeout}"; // Set timeout at page level
    vLabelDentalPolicyInfoError = '{!$LABEL.HUMWebActivityResponseErrors}';
    sPersonId ='{!JSENCODE(personID)}';
    </script>
    
    <!-- This block will render a spinner image to indicate table loading in the UI -->
    <div class="loadingImgStyle" id="statusImageLoading">
        <img src="/img/loading.gif"/>
        <span class="waitingDescription">{!$Label.LOADING_HUM}</span>
    </div>
    <div id="formId" class="mainDivstyle">
        <div id="responseErrors" class="responseErrorsStyle">
        </div>
            <apex:pageBlock id="DentalPolInfoPageBlock">
                <apex:pageBlockSection columns="2" id="DentalPolInfoPageBlockSection">  
                    <apex:pageBlockSectionItem id="PriorCarrierHeaderPBSI">
                    <apex:outputLabel value="{!$Label.PRIOR_CARRIER}" styleClass="GroupHeaderStyle"/>
                    </apex:pageBlockSectionItem>
                    <apex:pageBlockSectionItem />
                    <apex:pageBlockSectionItem id="DentalPolInfoPBSI1">
                        <apex:outputpanel >                            
                             <apex:outputLabel value="{!$Label.PRIOR_COV_INDICATOR}" style="font-size:12px;"/>
