                    </apex:pageBlockSectionItem>
                    <apex:pageBlockSectionItem id="DentalPolInfoPBSI13">
                        <apex:outputpanel >                            
                             <apex:outputLabel value="{!$Label.ENROLL_SOURCE}" style="font-size:12px;"/>
                        </apex:outputpanel>           
                             <span id="EnrollmentSource">
