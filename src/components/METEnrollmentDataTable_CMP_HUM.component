    <script src="{!URLFOR($Resource.CaseDetailResources_SR_HUM,'/CaseMET/METEnrollmentTable_JS_HUM.js')}"/>   
    <div>
        <c:BaseDataTable_CMP_HUM jqueryvariable="jQuery" 
                tableid="caseMETDatatable"
                enablesearch="f" 
                tablecontainerid="caseMETDatatableContainer"
