                info="true" 
                sort="false" 
                keyword="false"
                orderColumn="{!orderColumnNumber}"
                orderDirection="asc"
                pagingArray="[10,25,50,75,100]"
         />
    </div>
   <script>
       var sgetMETRecords = '{!$RemoteAction.METEnrollmentDataTable_C_HUM.getMETRecords}';
