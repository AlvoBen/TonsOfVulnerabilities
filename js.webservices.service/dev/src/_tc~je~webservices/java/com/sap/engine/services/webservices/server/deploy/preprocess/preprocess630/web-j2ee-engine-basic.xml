		<login-module-stack>
			<login-module>
				<login-module-name>com.sap.security.core.server.jaas.EvaluateTicketLoginModule</login-module-name>
				<flag>SUFFICIENT</flag>
			</login-module>
			<login-module>
				<login-module-name>com.sap.engine.services.security.server.jaas.BasicPasswordLoginModule</login-module-name>
