<?xml version="1.0" encoding="UTF-8" ?>
<ps:user-store-configuration xmlns:ps="http://schemas.xmlsoap.org/soap/encoding/"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://schemas.xmlsoap.org/soap/encoding/ userstore.xsd">
   <user-store>
	  <display-name>
	   DBMS User Store
	  </display-name>
	  <description>
		User Store that uses dbms.
	  </description>
	  <user-class-name>
	   com.sap.engine.services.userstore.UserContextImpl
	  </user-class-name>
	  <group-class-name>
	   com.sap.engine.services.userstore.GroupContextImpl
	  </group-class-name>
	  <anonymous-user>
		Guest
	  </anonymous-user>
  <login-module>
    <display-name>
      BasicPasswordLoginModule
    </display-name>
    <description>
      login module that verifies users' passwords
    </description>
    <suitable-mechanism>
      BASIC
    </suitable-mechanism>
    <suitable-mechanism>
      FORM
    </suitable-mechanism>
    <class-name>
      com.sap.engine.services.security.server.jaas.BasicPasswordLoginModule
    </class-name>
    <options-editor>
      com.sap.engine.services.security.gui.userstore.stores.EmptyOptionsEditor
    </options-editor>
  </login-module>
  <login-module>
    <display-name>
      ClientCertLoginModule
    </display-name>
    <description>
      login module used for client authentication
    </description>
    <suitable-mechanism>
      CLIENT_CERT
    </suitable-mechanism>
    <class-name>
      com.sap.engine.services.security.server.jaas.ClientCertLoginModule
    </class-name>
    <options Rule1.getUserFrom="wholeCert"/>
  </login-module>
  <login-module>
    <display-name>
      DigestLoginModule
    </display-name>
    <description>
      login module for digest authentication
    </description>
    <suitable-mechanism>
      DIGEST
    </suitable-mechanism>
    <class-name>
      com.sap.engine.services.security.server.jaas.DigestLoginModule
    </class-name>
    <options-editor>
      com.sap.engine.services.security.gui.userstore.stores.EmptyOptionsEditor
    </options-editor>
  </login-module>
  <login-module>
    <display-name>
      SAMLLoginModule
    </display-name>
    <description>
      Login module for SAML Browser/Artifact Profile
    </description>
    <suitable-mechanism>
      BASIC
    </suitable-mechanism>
    <suitable-mechanism>
      FORM
    </suitable-mechanism>
    <suitable-mechanism>
      CLIENT_CERT
    </suitable-mechanism>
    <suitable-mechanism>
      DIGEST
    </suitable-mechanism>
    <class-name>
      com.sap.security.core.server.saml.jaas.SAMLLoginModule
    </class-name>
    <options Mode                          = "Standalone" />
    <options AcceptedAuthenticationMethods = "*"          />
  </login-module>
  <login-module>
    <display-name>
      SecuritySessionLoginModule
    </display-name>
    <description>
      Login module used by download.ear. It uses tickets, generated by security service on the engine.
    </description>
    <class-name>
      com.sap.engine.services.security.server.jaas.SecuritySessionLoginModule
    </class-name>
    <options-editor>
      com.sap.engine.services.security.gui.userstore.stores.EmptyOptionsEditor
    </options-editor>
  </login-module>
  <login-module>
    <display-name>
      CSILoginModule
    </display-name>
    <description>
      login module for iiop service
    </description>
    <not-suitable-mechanism>
      BASIC
    </not-suitable-mechanism>
    <not-suitable-mechanism>
      FORM
    </not-suitable-mechanism>
    <not-suitable-mechanism>
      CLIENT_CERT
    </not-suitable-mechanism>
    <not-suitable-mechanism>
      DIGEST
    </not-suitable-mechanism>
    <class-name>
      com.sap.engine.lib.security.CSI.CSILoginModule
    </class-name>
  </login-module>
  <login-module>
    <display-name>
      CreateTicketLoginModule
    </display-name>
    <description>
      login module to create SAP Logon Tickets after successful logon
    </description>
    <suitable-mechanism>
      BASIC
    </suitable-mechanism>
    <suitable-mechanism>
      FORM
    </suitable-mechanism>
    <suitable-mechanism>
      CLIENT_CERT
    </suitable-mechanism>
    <suitable-mechanism>
      DIGEST
    </suitable-mechanism>
    <class-name>
      com.sap.security.core.server.jaas.CreateTicketLoginModule
    </class-name>
  </login-module>
  <login-module>
    <display-name>
      EvaluateTicketLoginModule
    </display-name>
    <description>
      login module to evaluate SAP Logon Tickets
    </description>
    <suitable-mechanism>
      BASIC
    </suitable-mechanism>
    <suitable-mechanism>
      FORM
    </suitable-mechanism>
    <suitable-mechanism>
      CLIENT_CERT
    </suitable-mechanism>
    <suitable-mechanism>
      DIGEST
    </suitable-mechanism>
    <class-name>
      com.sap.security.core.server.jaas.EvaluateTicketLoginModule
    </class-name>
  </login-module>
  <configuration RETRIEVE_PASSWORDS="false"/>
  <configuration LOCK_AFTER_INVALID_ATTEMPTS="5"/>
  <configuration anonymous-user="Guest"/>
  <startup-configuration userGuests.name="guests" userGuests.isUser="false" userGuests.password="" userGuests.parentGroups="all"/>
  <startup-configuration userAdmins.name="administrators" userAdmins.isUser="false" userAdmins.password="" userAdmins.parentGroups="all"/>
  <startup-configuration user1.name="Guest" user1.isUser="true" user1.password="sap" user1.parentGroups="guests" user1.isLocked="true"/>
  <startup-configuration user2.name="Administrator" user2.isUser="true" user2.password="sap" user2.parentGroups="administrators"/>
  </user-store>

<user-store active="yes">
 <display-name>
   UME User Store
 </display-name>
 <description>
   User Store that uses UME.
 </description>
 <user-class-name>
   com.sap.security.core.server.userstore.UserContextUME
 </user-class-name>
 <group-class-name>
   com.sap.security.core.server.userstore.GroupContextUME
 </group-class-name>
 <anonymous-user>
   Guest
 </anonymous-user>
 <login-module>
   <display-name>
     BasicPasswordLoginModule
   </display-name>
   <description>
     login module that verifies users' passwords
   </description>
   <suitable-mechanism>
     BASIC
   </suitable-mechanism>
   <suitable-mechanism>
     FORM
   </suitable-mechanism>
   <class-name>
     com.sap.engine.services.security.server.jaas.BasicPasswordLoginModule
   </class-name>
   <options-editor>
     com.sap.engine.services.security.gui.userstore.stores.EmptyOptionsEditor
   </options-editor>
 </login-module>
 <login-module>
   <display-name>
     ClientCertLoginModule
   </display-name>
   <description>
     login module used for client authentication
   </description>
   <suitable-mechanism>
     CLIENT_CERT
   </suitable-mechanism>
   <class-name>
     com.sap.engine.services.security.server.jaas.ClientCertLoginModule
   </class-name>
   <options subject=""/>
   <options issuer=""/>
   <options SerialNumber=""/>
   <options CertAuth=""/>
 </login-module>
 <login-module>
   <display-name>
     DigestLoginModule
   </display-name>
   <description>
     login module for digest authentication
   </description>
   <suitable-mechanism>
     DIGEST
   </suitable-mechanism>
   <class-name>
     com.sap.engine.services.security.server.jaas.DigestLoginModule
   </class-name>
   <options-editor>
     com.sap.engine.services.security.gui.userstore.stores.EmptyOptionsEditor
   </options-editor>
 </login-module>
 <login-module>
   <display-name>
     SAMLLoginModule
   </display-name>
   <description>
     Login module for SAML Browser/Artifact Profile
   </description>
   <suitable-mechanism>
     BASIC
   </suitable-mechanism>
   <suitable-mechanism>
     FORM
   </suitable-mechanism>
   <suitable-mechanism>
     CLIENT_CERT
   </suitable-mechanism>
   <suitable-mechanism>
     DIGEST
   </suitable-mechanism>
   <class-name>
     com.sap.security.core.server.saml.jaas.SAMLLoginModule
   </class-name>
   <options Mode                          = "Standalone" />
   <options AcceptedAuthenticationMethods = "*"          />
 </login-module>
 <login-module>
   <display-name>
     SecuritySessionLoginModule
   </display-name>
   <description>
     Login module used by download.ear. It uses tickets, generated by security service on the engine.
   </description>
   <class-name>
     com.sap.engine.services.security.server.jaas.SecuritySessionLoginModule
   </class-name>
   <options-editor>
     com.sap.engine.services.security.gui.userstore.stores.EmptyOptionsEditor
   </options-editor>
 </login-module>
 <login-module>
   <display-name>
     CSILoginModule
   </display-name>
   <description>
     login module for iiop service
   </description>
   <not-suitable-mechanism>
     BASIC
   </not-suitable-mechanism>
   <not-suitable-mechanism>
     FORM
   </not-suitable-mechanism>
   <not-suitable-mechanism>
     CLIENT_CERT
   </not-suitable-mechanism>
   <not-suitable-mechanism>
     DIGEST
   </not-suitable-mechanism>
   <class-name>
     com.sap.engine.lib.security.CSI.CSILoginModule
   </class-name>
 </login-module>
 <login-module>
   <display-name>
     CreateTicketLoginModule
   </display-name>
   <description>
     login module to create SAP Logon Tickets after successful logon
   </description>
   <suitable-mechanism>
     BASIC
   </suitable-mechanism>
   <suitable-mechanism>
     FORM
   </suitable-mechanism>
   <suitable-mechanism>
     CLIENT_CERT
   </suitable-mechanism>
   <suitable-mechanism>
     DIGEST
   </suitable-mechanism>
   <class-name>
     com.sap.security.core.server.jaas.CreateTicketLoginModule
   </class-name>
 </login-module>
 <login-module>
   <display-name>
     EvaluateTicketLoginModule
   </display-name>
   <description>
     login module to evaluate SAP Logon Tickets
   </description>
   <suitable-mechanism>
     BASIC
   </suitable-mechanism>
   <suitable-mechanism>
     FORM
   </suitable-mechanism>
   <suitable-mechanism>
     CLIENT_CERT
   </suitable-mechanism>
   <suitable-mechanism>
     DIGEST
   </suitable-mechanism>
   <class-name>
     com.sap.security.core.server.jaas.EvaluateTicketLoginModule
   </class-name>
 </login-module>
 <login-module>
   <display-name>
     AnonymousLoginModule
   </display-name>
   <description>
     Login module for named anonymous users.
   </description>
   <class-name>
     com.sap.security.core.server.jaas.AnonymousLoginModule 
   </class-name>
 </login-module>
 <login-module>
   <display-name>
     QueueAndResumeLoginModule
   </display-name>
   <description>
     Login module to resume a login for a queued message.
   </description>
   <class-name>
     com.sap.security.core.server.wssec.jaas.ResumeAsLoginModule
   </class-name>
   <options-editor>
     com.sap.engine.services.security.gui.userstore.stores.EmptyOptionsEditor
   </options-editor>
 </login-module>
 <configuration anonymous-user="Guest"/>
 <startup-configuration userGuests.name="Guests" userGuests.isUser="false" userGuests.password="" userGuests.parentGroups=""/>
 <startup-configuration userAdmins.name="Administrators" userAdmins.isUser="false" userAdmins.password="" userAdmins.parentGroups="" userAdmins.isAdmin="true"/>
 <startup-configuration user1.name="Guest" user1.isUser="true" user1.password="sap" user1.parentGroups="Guests" user1.isLocked="true"/>
 <startup-configuration user2.name="Administrator" user2.isUser="true" user2.password="sap" user2.parentGroups="Administrators"/>
 </user-store>

</ps:user-store-configuration>
